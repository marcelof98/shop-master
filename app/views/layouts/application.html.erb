<!DOCTYPE html>
<html>
  <head>
    <title>Sistema</title>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>

    <p id="notice"><%= notice %></p>

    <% if current_user %>
      <%= current_user.email %>.
      <%= link_to "Log Out", logout_path %>
    <% end %>

    <%= yield %>
  </body>
</html>
